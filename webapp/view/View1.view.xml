<mvc:View controllerName="vsdSAP-CreateRows.controller.View1" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns="sap.m">
	<App>
		<pages>
			<Page title="{i18n>title}">
				<content>
					<VBox>
						<Input type="Number" placeholder="Please Enter Positive Number.." id="inp" value="oJson>/InputValue" width="50%"></Input>
						<Button class="sapUiMediumMargin" text="Create" icon="sap-icon://create-form" width="20%"></Button>
						<Label id="lbl" design="Bold" ></Label>
						
						<html:hr></html:hr>
						
						<Table width="60%">
							<columns>
								<Column>
								
									<Text text="No"></Text>
								</Column>
								<Column>
									<Text text="Name"></Text>
								</Column>
							</columns>
						</Table>
					</VBox>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>